procedure get_data 
SET DIST=$P1
HEADER OFF
ARRAY INIT $(DIST) OUTDIST
OUTFIL 'HMS_SCRATCH.txt'
ARRAY TABLE OUTDIST
OUTFIL
CANCEL

procedure get_dist
SET DIST=$P1
HEADER OFF
PU INIT
PU MKARR $(DIST) OUT=OUTDIST DUM
OUTFIL 'HMS_SCRATCH.txt'
ARRAY TABLE OUTDIST
OUTFIL 
CANCEL

procedure get_var
HEADER OFF
SET VAR=$P1
PU INIT
PU SVAR $(VAR) QUIET
SET OUTVAR=$(XVAR_V)
OUTFIL 'HMS_SCRATCH.txt'
ECHO $(OUTVAR)
OUTFIL 
CANCEL

procedure distlist
OUTFIL 'HMS_SCRATCH.txt'
DIR
OUTFIL
CANCEL